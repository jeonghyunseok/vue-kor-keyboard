{
  "name": "vue-kor-keyboard",
  "version": "0.1.1",
  "description": "vue virtual qwerty keyboard template(Korean version)",
  "author": "jeong hyun seok",
  "license": "MIT",
  "hompage": "https://github.com/jeonghyunseok/vue-kor-keyboard",
